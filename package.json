{
  "name": "AiToolsZone",
  "private": true,
  "version": "0.0.1",
  "homepage": "https://msnandhis.github.io/aitoolszone2",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "predeploy": "node -e \"if(!require('fs').existsSync('.env')){console.error('Error: .env file is missing. Copy .env.example to .env and configure it.');process.exit(1)}\"",
    "deploy:build": "vite build",
    "deploy:upload": "node deploy.js",
    "deploy": "npm run predeploy && npm run deploy:build && npm run deploy:upload",
    "deploy:retry": "node deploy.js --retry-failed",
    "watch-deploy": "node watch-deploy.js",
    "validate:env": "node -e \"['FTP_HOST','FTP_USER','FTP_PASS'].forEach(v=>{if(!process.env[v]){console.error('Missing '+v+' in .env');process.exit(1)}})\""
  },
  "dependencies": {
    "@marsidev/react-turnstile": "^1.1.0",
    "@types/node": "^22.10.1",
    "@types/react-router-dom": "^5.3.3",
    "lucide-react": "^0.344.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "react-router-dom": "^7.0.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "basic-ftp": "^5.0.5",
    "chokidar": "^4.0.3",
    "dotenv": "^16.4.7",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "ftp-deploy": "^2.4.7",
    "gh-pages": "^6.2.0",
    "globals": "^15.9.0",
    "postcss": "^8.4.35",
    "simple-git": "^3.27.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2"
  }
}
